{
    "version": "2.0.0",
    "tasks": [
        {
            //> Task clean all file Lib: *.o, *.a, *.dll ...
            "label": "Clean_Lib",
            "dependsOn": [],
            "command": "clean_all.bat",
            "args": [
                "build"
            ],
        },
        {
            //> Task build all file code ...
            "label": "Build_All",
            "dependsOn": [],
            "command": "build_all.bat",
            "args": [
                "build"
            ],
        },
        {
            //> Task build file code changing ...
            "label": "Build_Changed",
            "dependsOn": [],
            "command": "Build_Changed.bat",
            "args": [
                "build"
            ],
        },
        {
            //> Task build default
            "label": "Build",
            "dependsOn": [
                "Clean_Lib",
                "Build_All",
                "Build_Changed",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}